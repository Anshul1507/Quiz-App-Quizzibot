<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quizzibot.com.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".start_screen"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        </activity>
        <activity
            android:name=".about_us"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".category"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".rules"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".random_quiz" />
        <activity android:name=".sports_quiz" />
        <activity android:name=".tech_quiz" />
        <activity android:name=".gk_quiz" />
        <activity android:name=".Results"></activity>
    </application>

</manifest>